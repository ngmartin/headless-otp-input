{
  "compilerOptions": {
    "jsx": "react",
    "target": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es2016", "dom"],
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "removeComments": false,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
  },
  "include": ["src"]
}
